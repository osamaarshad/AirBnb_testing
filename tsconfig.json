{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "types": ["cypress", "mocha"]  // Include Cypress and Mocha types
    },
    "include": [
    "cypress/**/*.ts",   // Include Cypress files for TS support
    "support/**/*.ts",
    "integration/**/*.ts"
  ],
    "exclude": [
      "node_modules"
    ]
  }
  
  